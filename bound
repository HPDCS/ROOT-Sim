scripts/Makefile:	      -e "s|^$$topsrcdirstrip/|$(top_builddir)/|;t"`; \
scripts/Makefile:			   sort -u` ;; \
scripts/Makefile:	  if test -f $$file || test -d $$file; then d=.; else d=$(srcdir); fi; \
scripts/Makefile:	  if test -d $$d/$$file; then \
scripts/Makefile:	    dir=`echo "/$$file" | sed -e 's,/[^/]*$$,,'`; \
scripts/Makefile:	    if test -d "$(distdir)/$$file"; then \
scripts/Makefile:	      find "$(distdir)/$$file" -type d ! -perm -700 -exec chmod u+rwx {} \;; \
scripts/Makefile:	    if test -d $(srcdir)/$$file && test $$d != $(srcdir); then \
scripts/Makefile:	      cp -fpR $(srcdir)/$$file "$(distdir)$$dir" || exit 1; \
scripts/Makefile:	      find "$(distdir)/$$file" -type d ! -perm -700 -exec chmod u+rwx {} \;; \
scripts/Makefile:	    cp -fpR $$d/$$file "$(distdir)$$dir" || exit 1; \
scripts/Makefile:	    test -f "$(distdir)/$$file" \
scripts/Makefile:	    || cp -p $$d/$$file "$(distdir)/$$file" \
