scripts/Makefile.in:.PHONY: rootsim-cc
scripts/Makefile.in:rootsim-cc: rootsim-cc.in
scripts/Makefile.in:	@$(do_subst) < rootsim-cc.in > rootsim-cc.pre
scripts/Makefile.in:	@cat rootsim-cc.pre ld-data1 ld-data2 ld-final > rootsim-cc
scripts/Makefile.in:	@rm rootsim-cc.pre
scripts/Makefile.in:	@chmod +x rootsim-cc
scripts/rootsim-cc:# ROOT-Sim CC - Copyright (C) HPDCS Group @ Sapienza, University of Rome
scripts/rootsim-cc:#   22 sep 2012 - Alessandro Pellegrini <pellegrini@dis.uniroma1.it>
scripts/rootsim-cc:# ROOT-Sim CC: this is just a gcc/mpicc wrapper which follows through the
scripts/rootsim-cc:# the static ROOT-Sim Library
scripts/rootsim-cc:CFLAGS="-O3 -lm -U_FORTIFY_SOURCE -pthread"
scripts/rootsim-cc:This is the ROOT-Sim compiler.
scripts/rootsim-cc:It allows compiling C sources against the ROOT-Sim library.
scripts/rootsim-cc:   -h    Print this help screen
scripts/rootsim-cc:   -c    Do not link against the ROOT-Sim library (multistage compilation)
scripts/rootsim-cc:   -o N  Specify N as output name
scripts/rootsim-cc:can be safely passed to rootsim-cc
scripts/rootsim-cc:tail -n +$START_LINES "$0" | head -n +$LD_DATA1_LINES > $$.ld-data1
scripts/rootsim-cc:tail -n +$START_LINES "$0" | head -n +$LD_DATA2_LINES > $$.ld-data2
scripts/rootsim-cc:tail -n +$START_LINES "$0" | head -n +$LD_FINAL_LINES > $$.ld-final
scripts/rootsim-cc:#rm $$.ld-data1
scripts/rootsim-cc:#rm $$.ld-data2
scripts/rootsim-cc:#rm $$.ld-final
scripts/rootsim-cc:#rm branch-table
scripts/rootsim-cc:#rm branch-table-light
scripts/rootsim-cc:#rm offset-table-start-inc
scripts/rootsim-cc:#unlink offset-table-start-full # :)
scripts/rootsim-cc:#rm monitor-table
scripts/rootsim-cc:#rm offset-table
scripts/rootsim-cc:#rm offset-table-light
scripts/rootsim-cc:#rm padding-monitor.c
scripts/rootsim-cc:#rm riferimenti-mon
scripts/rootsim-cc:if [ $? -ne 0 ]; then
scripts/rootsim-cc:# The ROOT-Sim CC script begins here
scripts/rootsim-cc:CC="$CC -L $libdir -I $includedir"   # This is how we will call the compiler.
scripts/rootsim-cc:		-c)
scripts/rootsim-cc:				CFLAGS="$CFLAGS -c"
scripts/rootsim-cc:		-o)
scripts/rootsim-cc:		-MT)
scripts/rootsim-cc:			CFLAGS="$CFLAGS -MT"
scripts/rootsim-cc:		-MF)
scripts/rootsim-cc:			CFLAGS="$CFLAGS -MF"
scripts/rootsim-cc:			elif [ "${word:0:1}" = "-" ]; then
scripts/rootsim-cc:if [ ! -n "$SOURCES" ]; then
scripts/rootsim-cc:# -c is actually included into CFLAGS
scripts/rootsim-cc:	$CC $CFLAGS $SOURCES -DOnGVT=OnGVT_light -DProcessEvent=ProcessEvent_light -o $OUTNAME
scripts/rootsim-cc:#### Ok, no partial compilation. Now start the loooong compilation procedure for ROOT-Sim
scripts/rootsim-cc:	if [ "${f: -2:2}" = ".o" ]; then
scripts/rootsim-cc:		$CC $CFLAGS -c $f -DOnGVT=OnGVT_light -DProcessEvent=ProcessEvent_light -o ${f/.c/.o.rs}
scripts/rootsim-cc:$LD -r ${OBJECTS//.o/.o.rs} -o APP.o
scripts/rootsim-cc:cp APP.o APP-original.o
scripts/rootsim-cc:#cp APP.o inserito-simbolo
scripts/rootsim-cc:#$CC padding.c -c -o APP-padding.o
scripts/rootsim-cc:#$LD -r APP.o APP-padding.o -o APP-padded.o
scripts/rootsim-cc:#mv APP-padded.o APP.o
scripts/rootsim-cc:#$bindir/hijacker_load_table monitor.o monitor-table .insn_table 0
scripts/rootsim-cc:#$bindir/hijacker_load_table APP.o branch-table .warp `cat offset-table-start-inc`
scripts/rootsim-cc:#$bindir/hijacker_load_table branch.o offset-table .offset_table 0
scripts/rootsim-cc:#$LD -r branch.o brn.o -o branch-final.o
scripts/rootsim-cc:#mv branch-final.o branch.o
scripts/rootsim-cc:#$CC padding-monitor.c -c -o padding-monitor.o
scripts/rootsim-cc:#$LD -r monitor.o padding-monitor.o -o monitor-padded.o
scripts/rootsim-cc:#$bindir/hijacker_relocator monitor-padded.o riferimenti-mon .insn_table
scripts/rootsim-cc:#$LD -r APP.o mon.o monitor-padded.o branch.o -o APP-incremental.o
scripts/rootsim-cc:### Now instrument the code for the non-incremental (light) version
scripts/rootsim-cc:#$bindir/hijacker_instrument_light APP-original.o $datadir/hijacker/monitor/ .
scripts/rootsim-cc:#$CC padding.c -c -o padding.o
scripts/rootsim-cc:#$LD -r APP-original.o padding.o -o APP.o
scripts/rootsim-cc:#$bindir/hijacker_load_table APP.o branch-table-light .warl `cat offset-table-start-full`
scripts/rootsim-cc:#$bindir/hijacker_load_table branch.o offset-table-light .offset_table_l 0
scripts/rootsim-cc:#$LD -r branch.o brn.o -o branch-final.o
scripts/rootsim-cc:#mv branch-final.o branch.o
scripts/rootsim-cc:#$LD -r APP.o branch.o -o APP-non-incremental.o
scripts/rootsim-cc:### Now rename application callback's so that ROOT-Sim is able to call them.
scripts/rootsim-cc:#$bindir/hijacker_rename_functions APP-incremental.o _inc
scripts/rootsim-cc:#$bindir/hijacker_rename_functions APP-non-incremental.o _light
scripts/rootsim-cc:#$LD -r -T $$.ld-data1 APP-incremental.o -o APP-incremental-resectioned.o
scripts/rootsim-cc:#$LD -r -T $$.ld-data2 APP-non-incremental.o -o APP-non-incremental-resectioned.o
scripts/rootsim-cc:#$LD -r -z muldefs APP-incremental-resectioned.o APP-non-incremental-resectioned.o -o APP-final.o
scripts/rootsim-cc:### the ROOT-Sim library.
scripts/rootsim-cc:###   4. Link to the remaining ROOT-Sim library: this will produce the final executable
scripts/rootsim-cc:$LD -r -L $libdir --wrap strcpy --wrap strncpy --wrap strcat --wrap strncat --wrap memcpy --wrap memmove --wrap memset --wrap strdup --wrap strndup -o APP-lib-wrapped.o APP.o -lwrapperl
scripts/rootsim-cc:$LD -r -L $libdir --wrap malloc --wrap free --wrap realloc --wrap calloc -o APP-dym-wrapped.o APP-lib-wrapped.o --whole-archive -ldymelor
scripts/rootsim-cc:#$CC -Xlinker --no-check-sections -Xlinker -T -Xlinker $$.ld-final -Xlinker --section-start -Xlinker .warp=$APPLICATION_START_POINT -Xlinker --section-start -Xlinker .warl=$APPLICATION_START_POINT_LIGHT  APP-dym-wrapped.o -lrootsim -lm -DGATHER_DATA -o $OUTNAME
scripts/rootsim-cc:$CC APP-dym-wrapped.o -lrootsim $CFLAGS -lm -DGATHER_DATA -o $OUTNAME
scripts/rootsim-cc:        APP-incremental.o (.data)
scripts/rootsim-cc:        APP-incremental.o (.data1)
scripts/rootsim-cc:        APP-incremental.o (.bss)
scripts/rootsim-cc:        APP-incremental.o (.rodata)
scripts/rootsim-cc:        APP-incremental.o (.rodata1)
scripts/rootsim-cc:        APP-non-incremental.o (.data)
scripts/rootsim-cc:        APP-non-incremental.o (.data1)
scripts/rootsim-cc:        APP-non-incremental.o (.bss)
scripts/rootsim-cc:        APP-non-incremental.o (.rodata)
scripts/rootsim-cc:        APP-non-incremental.o (.rodata1)
scripts/rootsim-cc:OUTPUT_FORMAT("elf64-x86-64", "elf64-x86-64",
scripts/rootsim-cc:	      "elf64-x86-64")
scripts/rootsim-cc:OUTPUT_ARCH(i386:x86-64)
scripts/rootsim-cc:SEARCH_DIR("/usr/x86_64-suse-linux/lib64"); SEARCH_DIR("/usr/local/lib64"); SEARCH_DIR("/lib64"); SEARCH_DIR("/usr/lib64"); SEARCH_DIR("/usr/x86_64-suse-linux/lib"); SEARCH_DIR("/usr/lib64"); SEARCH_DIR("/usr/local/lib"); SEARCH_DIR("/lib"); SEARCH_DIR("/usr/lib");
scripts/rootsim-cc:  /* Read-only sections, merged into text segment: */
scripts/rootsim-cc:  . = ALIGN (CONSTANT (MAXPAGESIZE)) - ((CONSTANT (MAXPAGESIZE) - .) & (CONSTANT (MAXPAGESIZE) - 1)); . = DATA_SEGMENT_ALIGN (CONSTANT (MAXPAGESIZE), CONSTANT (COMMONPAGESIZE));
scripts/rootsim-cc:  .lrodata   ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/rootsim-cc:  .ldata   ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/rootsim-cc:    .data_i ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/rootsim-cc:    .bss_i ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/rootsim-cc:    .rodata_i ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/rootsim-cc:    .rodata_l ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/rootsim-cc:    .monitor_var ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/rootsim-cc:  /DISCARD/ : { *(.note.GNU-stack) *(.gnu_debuglink) }
scripts/Makefile:# Copyright (C) 1994-2013 Free Software Foundation, Inc.
scripts/Makefile:am__is_gnu_make = test -n '$(MAKEFILE_LIST)' && test -n '$(MAKELEVEL)'
scripts/Makefile:  case $${target_option-} in \
scripts/Makefile:              "target option '$${target_option-}' specified" >&2; \
scripts/Makefile:      *=*|--*) continue;; \
scripts/Makefile:        -*I) strip_trailopt 'I'; skip_next=yes;; \
scripts/Makefile:      -*I?*) strip_trailopt 'I';; \
scripts/Makefile:        -*O) strip_trailopt 'O'; skip_next=yes;; \
scripts/Makefile:      -*O?*) strip_trailopt 'O';; \
scripts/Makefile:        -*l) strip_trailopt 'l'; skip_next=yes;; \
scripts/Makefile:      -*l?*) strip_trailopt 'l';; \
scripts/Makefile:      -[dEDm]) skip_next=yes;; \
scripts/Makefile:      -[JT]) skip_next=yes;; \
scripts/Makefile:install_sh_DATA = $(install_sh) -c -m 644
scripts/Makefile:install_sh_PROGRAM = $(install_sh) -c
scripts/Makefile:install_sh_SCRIPT = $(install_sh) -c
scripts/Makefile:build_triplet = x86_64-suse-linux-gnu
scripts/Makefile:host_triplet = x86_64-suse-linux-gnu
scripts/Makefile:mkinstalldirs = $(install_sh) -d
scripts/Makefile:am__strip_dir = f=`echo $$p | sed -e 's|^.*/||'`;
scripts/Makefile:  for p in $$list; do echo "$$p"; done | sed -e "s|$$srcdirstrip/||"
scripts/Makefile:  test -z "$$files" \
scripts/Makefile:    || { test ! -d "$$dir" && test ! -f "$$dir" && test ! -r "$$dir"; } \
scripts/Makefile:    || { echo " ( cd '$$dir' && rm -f" $$files ")"; \
scripts/Makefile:         $(am__cd) "$$dir" && rm -f $$files; }; \
scripts/Makefile:    *) (install-info --version) >/dev/null 2>&1;; \
scripts/Makefile:ACLOCAL = ${SHELL} /home/rootsim/Repository/ROOT-Sim/missing aclocal-1.13
scripts/Makefile:AMTAR = $${TAR-tar}
scripts/Makefile:AUTOCONF = ${SHELL} /home/rootsim/Repository/ROOT-Sim/missing autoconf
scripts/Makefile:AUTOHEADER = ${SHELL} /home/rootsim/Repository/ROOT-Sim/missing autoheader
scripts/Makefile:AUTOMAKE = ${SHELL} /home/rootsim/Repository/ROOT-Sim/missing automake-1.13
scripts/Makefile:CCASFLAGS = -O3 -lm -U_FORTIFY_SOURCE
