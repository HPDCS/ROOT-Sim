scripts/Makefile.in:  for p in $$list; do echo "$$p"; done | sed -e "s|$$srcdirstrip/||"
scripts/Makefile.in:  test -z "$$files" \
scripts/Makefile.in:    || { test ! -d "$$dir" && test ! -f "$$dir" && test ! -r "$$dir"; } \
scripts/Makefile.in:    || { echo " ( cd '$$dir' && rm -f" $$files ")"; \
scripts/Makefile.in:         $(am__cd) "$$dir" && rm -f $$files; }; \
scripts/Makefile.in:    *) (install-info --version) >/dev/null 2>&1;; \
