src/arch/linux/modules/Makefile.in:	      cp -fpR $(srcdir)/$$file "$(distdir)$$dir" || exit 1; \
src/arch/linux/modules/Makefile.in:	      find "$(distdir)/$$file" -type d ! -perm -700 -exec chmod u+rwx {} \;; \
src/arch/linux/modules/Makefile.in:	    cp -fpR $$d/$$file "$(distdir)$$dir" || exit 1; \
src/arch/linux/modules/Makefile.in:	    test -f "$(distdir)/$$file" \
src/arch/linux/modules/Makefile.in:	    || cp -p $$d/$$file "$(distdir)/$$file" \
