Makefile:        dir2=`echo "$$dir0" | sed -e "$$sed_last"`/"$$dir2"; \
Makefile:        dir0=`echo "$$dir0" | sed -e "$$sed_butlast"`; \
Makefile:        first2=`echo "$$dir2" | sed -e "$$sed_first"`; \
Makefile:          dir2=`echo "$$dir2" | sed -e "$$sed_rest"`; \
