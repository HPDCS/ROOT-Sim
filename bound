scripts/Makefile:	if test -n "$$list"; then \
scripts/Makefile:	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
scripts/Makefile:	  if test -f "$$d$$p"; then echo "$$d$$p"; echo "$$p"; else :; fi; \
scripts/Makefile:	sed -e 'p;s,.*/,,;n' \
scripts/Makefile:	    -e 'h;s|.*|.|' \
scripts/Makefile:	    -e 'p;x;s,.*/,,;$(transform)' | sed 'N;N;N;s,\n, ,g' | \
scripts/Makefile:	     test -z "$$files" || { \
