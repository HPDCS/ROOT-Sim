scripts/Makefile.am:ACLOCAL_AMFLAGS = -I m4 
scripts/Makefile.am:AM_MAKEFLAGS = --no-print-directory
scripts/Makefile.am:.PHONY: rootsim-cc
scripts/Makefile.am:dist_bin_SCRIPTS = rootsim-cc
scripts/Makefile.am:EXTRA_DIST = ld-data1 ld-data2 ld-final
scripts/Makefile.am:do_subst = $(SED)	-e 's,[@]bindir[@],$(bindir),g' \
scripts/Makefile.am:	   	  	-e 's,[@]libdir[@],$(libdir),g' \
scripts/Makefile.am:			-e 's,[@]includedir[@],$(includedir),g' \
scripts/Makefile.am:			-e 's,[@]datadir[@],$(datadir),g' \
scripts/Makefile.am:			-e 's,[@]VERSION[@],$(VERSION),g' \
scripts/Makefile.am:			-e 's,[@]VERSION[@],$(VERSION),g' \
scripts/Makefile.am:			-e 's,[@]CC[@],$(CC),g' \
scripts/Makefile.am:			-e 's,[@]CFLAGS[@],$(CFLAGS),g' \
scripts/Makefile.am:			-e 's,[@]ARCH[@],$(ARCH),g' \
scripts/Makefile.am:			-e 's,[@]scriptlines[@],$(SCRIPT_LINES),g' \
scripts/Makefile.am:			-e 's,[@]lddata1lines[@],$(LD_DATA1_LINES),g' \
scripts/Makefile.am:			-e 's,[@]lddata2lines[@],$(LD_DATA2_LINES),g' \
scripts/Makefile.am:			-e 's,[@]ldfinallines[@],$(LD_FINAL_LINES),g'
scripts/Makefile.am:rootsim-cc: rootsim-cc.in
scripts/Makefile.am:	@$(do_subst) < rootsim-cc.in > rootsim-cc.pre
scripts/Makefile.am:	@cat rootsim-cc.pre ld-data1 ld-data2 ld-final > rootsim-cc
scripts/Makefile.am:	@rm rootsim-cc.pre
scripts/Makefile.am:	@chmod +x rootsim-cc
scripts/ld-data1:        APP-incremental.o (.data)
scripts/ld-data1:        APP-incremental.o (.data1)
scripts/ld-data1:        APP-incremental.o (.bss)
scripts/ld-data1:        APP-incremental.o (.rodata)
scripts/ld-data1:        APP-incremental.o (.rodata1)
scripts/ld-data2:        APP-non-incremental.o (.data)
scripts/ld-data2:        APP-non-incremental.o (.data1)
scripts/ld-data2:        APP-non-incremental.o (.bss)
scripts/ld-data2:        APP-non-incremental.o (.rodata)
scripts/ld-data2:        APP-non-incremental.o (.rodata1)
scripts/ld-final:OUTPUT_FORMAT("elf64-x86-64", "elf64-x86-64",
scripts/ld-final:	      "elf64-x86-64")
scripts/ld-final:OUTPUT_ARCH(i386:x86-64)
scripts/ld-final:SEARCH_DIR("/usr/x86_64-suse-linux/lib64"); SEARCH_DIR("/usr/local/lib64"); SEARCH_DIR("/lib64"); SEARCH_DIR("/usr/lib64"); SEARCH_DIR("/usr/x86_64-suse-linux/lib"); SEARCH_DIR("/usr/lib64"); SEARCH_DIR("/usr/local/lib"); SEARCH_DIR("/lib"); SEARCH_DIR("/usr/lib");
scripts/ld-final:  /* Read-only sections, merged into text segment: */
scripts/ld-final:  . = ALIGN (CONSTANT (MAXPAGESIZE)) - ((CONSTANT (MAXPAGESIZE) - .) & (CONSTANT (MAXPAGESIZE) - 1)); . = DATA_SEGMENT_ALIGN (CONSTANT (MAXPAGESIZE), CONSTANT (COMMONPAGESIZE));
scripts/ld-final:  .lrodata   ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/ld-final:  .ldata   ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/ld-final:    .data_i ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/ld-final:    .bss_i ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/ld-final:    .rodata_i ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/ld-final:    .rodata_l ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/ld-final:    .monitor_var ALIGN(CONSTANT (MAXPAGESIZE)) + (. & (CONSTANT (MAXPAGESIZE) - 1)) :
scripts/ld-final:  /DISCARD/ : { *(.note.GNU-stack) *(.gnu_debuglink) }
scripts/rootsim-cc.in:# ROOT-Sim CC - Copyright (C) HPDCS Group @ Sapienza, University of Rome
scripts/rootsim-cc.in:#   22 sep 2012 - Alessandro Pellegrini <pellegrini@dis.uniroma1.it>
scripts/rootsim-cc.in:# ROOT-Sim CC: this is just a gcc/mpicc wrapper which follows through the
scripts/rootsim-cc.in:# the static ROOT-Sim Library
scripts/rootsim-cc.in:This is the ROOT-Sim compiler.
scripts/rootsim-cc.in:It allows compiling C sources against the ROOT-Sim library.
scripts/rootsim-cc.in:   -h    Print this help screen
scripts/rootsim-cc.in:   -c    Do not link against the ROOT-Sim library (multistage compilation)
scripts/rootsim-cc.in:   -o N  Specify N as output name
scripts/rootsim-cc.in:can be safely passed to rootsim-cc
scripts/rootsim-cc.in:tail -n +$START_LINES "$0" | head -n +$LD_DATA1_LINES > $$.ld-data1
scripts/rootsim-cc.in:tail -n +$START_LINES "$0" | head -n +$LD_DATA2_LINES > $$.ld-data2
scripts/rootsim-cc.in:tail -n +$START_LINES "$0" | head -n +$LD_FINAL_LINES > $$.ld-final
scripts/rootsim-cc.in:#rm $$.ld-data1
scripts/rootsim-cc.in:#rm $$.ld-data2
scripts/rootsim-cc.in:#rm $$.ld-final
scripts/rootsim-cc.in:#rm branch-table
scripts/rootsim-cc.in:#rm branch-table-light
scripts/rootsim-cc.in:#rm offset-table-start-inc
scripts/rootsim-cc.in:#unlink offset-table-start-full # :)
scripts/rootsim-cc.in:#rm monitor-table
scripts/rootsim-cc.in:#rm offset-table
scripts/rootsim-cc.in:#rm offset-table-light
scripts/rootsim-cc.in:#rm padding-monitor.c
scripts/rootsim-cc.in:#rm riferimenti-mon
scripts/rootsim-cc.in:if [ $? -ne 0 ]; then
scripts/rootsim-cc.in:# The ROOT-Sim CC script begins here
scripts/rootsim-cc.in:CC="$CC -L $libdir -I $includedir"   # This is how we will call the compiler.
scripts/rootsim-cc.in:		-c)
scripts/rootsim-cc.in:				CFLAGS="$CFLAGS -c"
scripts/rootsim-cc.in:		-o)
scripts/rootsim-cc.in:		-MT)
scripts/rootsim-cc.in:			CFLAGS="$CFLAGS -MT"
scripts/rootsim-cc.in:		-MF)
scripts/rootsim-cc.in:			CFLAGS="$CFLAGS -MF"
scripts/rootsim-cc.in:			elif [ "${word:0:1}" = "-" ]; then
scripts/rootsim-cc.in:if [ ! -n "$SOURCES" ]; then
scripts/rootsim-cc.in:# -c is actually included into CFLAGS
scripts/rootsim-cc.in:	$CC $CFLAGS $SOURCES -DOnGVT=OnGVT_light -DProcessEvent=ProcessEvent_light -o $OUTNAME
scripts/rootsim-cc.in:#### Ok, no partial compilation. Now start the loooong compilation procedure for ROOT-Sim
scripts/rootsim-cc.in:	if [ "${f: -2:2}" = ".o" ]; then
scripts/rootsim-cc.in:		$CC $CFLAGS -c $f -DOnGVT=OnGVT_light -DProcessEvent=ProcessEvent_light -o ${f/.c/.o.rs}
scripts/rootsim-cc.in:$LD -r ${OBJECTS//.o/.o.rs} -o APP.o
scripts/rootsim-cc.in:cp APP.o APP-original.o
scripts/rootsim-cc.in:#cp APP.o inserito-simbolo
scripts/rootsim-cc.in:#$CC padding.c -c -o APP-padding.o
scripts/rootsim-cc.in:#$LD -r APP.o APP-padding.o -o APP-padded.o
scripts/rootsim-cc.in:#mv APP-padded.o APP.o
scripts/rootsim-cc.in:#$bindir/hijacker_load_table monitor.o monitor-table .insn_table 0
scripts/rootsim-cc.in:#$bindir/hijacker_load_table APP.o branch-table .warp `cat offset-table-start-inc`
scripts/rootsim-cc.in:#$bindir/hijacker_load_table branch.o offset-table .offset_table 0
scripts/rootsim-cc.in:#$LD -r branch.o brn.o -o branch-final.o
scripts/rootsim-cc.in:#mv branch-final.o branch.o
scripts/rootsim-cc.in:#$CC padding-monitor.c -c -o padding-monitor.o
scripts/rootsim-cc.in:#$LD -r monitor.o padding-monitor.o -o monitor-padded.o
scripts/rootsim-cc.in:#$bindir/hijacker_relocator monitor-padded.o riferimenti-mon .insn_table
scripts/rootsim-cc.in:#$LD -r APP.o mon.o monitor-padded.o branch.o -o APP-incremental.o
scripts/rootsim-cc.in:### Now instrument the code for the non-incremental (light) version
scripts/rootsim-cc.in:#$bindir/hijacker_instrument_light APP-original.o $datadir/hijacker/monitor/ .
scripts/rootsim-cc.in:#$CC padding.c -c -o padding.o
scripts/rootsim-cc.in:#$LD -r APP-original.o padding.o -o APP.o
scripts/rootsim-cc.in:#$bindir/hijacker_load_table APP.o branch-table-light .warl `cat offset-table-start-full`
scripts/rootsim-cc.in:#$bindir/hijacker_load_table branch.o offset-table-light .offset_table_l 0
scripts/rootsim-cc.in:#$LD -r branch.o brn.o -o branch-final.o
scripts/rootsim-cc.in:#mv branch-final.o branch.o
scripts/rootsim-cc.in:#$LD -r APP.o branch.o -o APP-non-incremental.o
scripts/rootsim-cc.in:### Now rename application callback's so that ROOT-Sim is able to call them.
scripts/rootsim-cc.in:#$bindir/hijacker_rename_functions APP-incremental.o _inc
scripts/rootsim-cc.in:#$bindir/hijacker_rename_functions APP-non-incremental.o _light
scripts/rootsim-cc.in:#$LD -r -T $$.ld-data1 APP-incremental.o -o APP-incremental-resectioned.o
scripts/rootsim-cc.in:#$LD -r -T $$.ld-data2 APP-non-incremental.o -o APP-non-incremental-resectioned.o
scripts/rootsim-cc.in:#$LD -r -z muldefs APP-incremental-resectioned.o APP-non-incremental-resectioned.o -o APP-final.o
scripts/rootsim-cc.in:### the ROOT-Sim library.
scripts/rootsim-cc.in:###   4. Link to the remaining ROOT-Sim library: this will produce the final executable
scripts/rootsim-cc.in:$LD -r -L $libdir --wrap strcpy --wrap strncpy --wrap strcat --wrap strncat --wrap memcpy --wrap memmove --wrap memset --wrap strdup --wrap strndup -o APP-lib-wrapped.o APP.o -lwrapperl
scripts/rootsim-cc.in:$LD -r -L $libdir --wrap malloc --wrap free --wrap realloc --wrap calloc -o APP-dym-wrapped.o APP-lib-wrapped.o --whole-archive -ldymelor
scripts/rootsim-cc.in:#$CC -Xlinker --no-check-sections -Xlinker -T -Xlinker $$.ld-final -Xlinker --section-start -Xlinker .warp=$APPLICATION_START_POINT -Xlinker --section-start -Xlinker .warl=$APPLICATION_START_POINT_LIGHT  APP-dym-wrapped.o -lrootsim -lm -DGATHER_DATA -o $OUTNAME
scripts/rootsim-cc.in:$CC APP-dym-wrapped.o -lrootsim $CFLAGS -lm -DGATHER_DATA -o $OUTNAME
scripts/Makefile.in:# Copyright (C) 1994-2013 Free Software Foundation, Inc.
scripts/Makefile.in:am__is_gnu_make = test -n '$(MAKEFILE_LIST)' && test -n '$(MAKELEVEL)'
