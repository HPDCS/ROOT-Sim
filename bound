Makefile:			   sort -u` ;; \
Makefile:	  if test -f $$file || test -d $$file; then d=.; else d=$(srcdir); fi; \
Makefile:	  if test -d $$d/$$file; then \
Makefile:	    dir=`echo "/$$file" | sed -e 's,/[^/]*$$,,'`; \
Makefile:	    if test -d "$(distdir)/$$file"; then \
Makefile:	      find "$(distdir)/$$file" -type d ! -perm -700 -exec chmod u+rwx {} \;; \
Makefile:	    if test -d $(srcdir)/$$file && test $$d != $(srcdir); then \
Makefile:	      cp -fpR $(srcdir)/$$file "$(distdir)$$dir" || exit 1; \
Makefile:	      find "$(distdir)/$$file" -type d ! -perm -700 -exec chmod u+rwx {} \;; \
Makefile:	    cp -fpR $$d/$$file "$(distdir)$$dir" || exit 1; \
Makefile:	    test -f "$(distdir)/$$file" \
Makefile:	    || cp -p $$d/$$file "$(distdir)/$$file" \
Makefile:	      || test -d "$(distdir)/$$subdir" \
Makefile:	-test -n "$(am__skip_mode_fix)" \
Makefile:	|| find "$(distdir)" -type d ! -perm -755 \
Makefile:		-exec chmod u+rwx,go+rx {} \; -o \
Makefile:	  ! -type d ! -perm -444 -links 1 -exec chmod a+r {} \; -o \
Makefile:	  ! -type d ! -perm -400 -exec chmod a+r {} \; -o \
Makefile:	  ! -type d ! -perm -444 -exec $(install_sh) -c -m a+r {} {} \; \
Makefile:	|| chmod -R a+r "$(distdir)"
Makefile:dist-gzip: distdir
Makefile:	tardir=$(distdir) && $(am__tar) | GZIP=$(GZIP_ENV) gzip -c >$(distdir).tar.gz
Makefile:dist-bzip2: distdir
Makefile:	tardir=$(distdir) && $(am__tar) | BZIP2=$${BZIP2--9} bzip2 -c >$(distdir).tar.bz2
Makefile:dist-lzip: distdir
Makefile:	tardir=$(distdir) && $(am__tar) | lzip -c $${LZIP_OPT--9} >$(distdir).tar.lz
Makefile:dist-xz: distdir
Makefile:	tardir=$(distdir) && $(am__tar) | XZ_OPT=$${XZ_OPT--e} xz -c >$(distdir).tar.xz
Makefile:dist-tarZ: distdir
Makefile:	tardir=$(distdir) && $(am__tar) | compress -c >$(distdir).tar.Z
Makefile:dist-shar: distdir
Makefile:	shar $(distdir) | GZIP=$(GZIP_ENV) gzip -c >$(distdir).shar.gz
Makefile:dist-zip: distdir
Makefile:	-rm -f $(distdir).zip
Makefile:	zip -rq $(distdir).zip $(distdir)
Makefile:dist dist-all:
Makefile:# it guarantees that the distribution is self-contained by making another
Makefile:	  GZIP=$(GZIP_ENV) gzip -dc $(distdir).tar.gz | $(am__untar) ;;\
Makefile:	  bzip2 -dc $(distdir).tar.bz2 | $(am__untar) ;;\
Makefile:	  lzip -dc $(distdir).tar.lz | $(am__untar) ;;\
Makefile:	  xz -dc $(distdir).tar.xz | $(am__untar) ;;\
Makefile:	  uncompress -c $(distdir).tar.Z | $(am__untar) ;;\
Makefile:	  GZIP=$(GZIP_ENV) gzip -dc $(distdir).shar.gz | unshar ;;\
Makefile:	chmod -R a-w $(distdir)
