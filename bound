scripts/Makefile.in:	      -e "s|^$$topsrcdirstrip/|$(top_builddir)/|;t"`; \
scripts/Makefile.in:			   sort -u` ;; \
scripts/Makefile.in:	  if test -f $$file || test -d $$file; then d=.; else d=$(srcdir); fi; \
scripts/Makefile.in:	  if test -d $$d/$$file; then \
scripts/Makefile.in:	    dir=`echo "/$$file" | sed -e 's,/[^/]*$$,,'`; \
scripts/Makefile.in:	    if test -d "$(distdir)/$$file"; then \
scripts/Makefile.in:	      find "$(distdir)/$$file" -type d ! -perm -700 -exec chmod u+rwx {} \;; \
scripts/Makefile.in:	    if test -d $(srcdir)/$$file && test $$d != $(srcdir); then \
scripts/Makefile.in:	      cp -fpR $(srcdir)/$$file "$(distdir)$$dir" || exit 1; \
scripts/Makefile.in:	      find "$(distdir)/$$file" -type d ! -perm -700 -exec chmod u+rwx {} \;; \
scripts/Makefile.in:	    cp -fpR $$d/$$file "$(distdir)$$dir" || exit 1; \
scripts/Makefile.in:	    test -f "$(distdir)/$$file" \
scripts/Makefile.in:	    || cp -p $$d/$$file "$(distdir)/$$file" \
