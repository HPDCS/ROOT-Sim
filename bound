scripts/Makefile.in:	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
scripts/Makefile.in:	  if test -f "$$d$$p"; then echo "$$d$$p"; echo "$$p"; else :; fi; \
scripts/Makefile.in:	sed -e 'p;s,.*/,,;n' \
scripts/Makefile.in:	    -e 'h;s|.*|.|' \
scripts/Makefile.in:	    -e 'p;x;s,.*/,,;$(transform)' | sed 'N;N;N;s,\n, ,g' | \
scripts/Makefile.in:	     test -z "$$files" || { \
scripts/Makefile.in:uninstall-dist_binSCRIPTS:
scripts/Makefile.in:	@list='$(dist_bin_SCRIPTS)'; test -n "$(bindir)" || exit 0; \
scripts/Makefile.in:	       sed -e 's,.*/,,;$(transform)'`; \
